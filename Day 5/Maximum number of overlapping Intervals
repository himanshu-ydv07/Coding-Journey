class Solution {
  public:
    int overlapInt(vector<vector<int>> &arr) {
        map<int, int> timeline;

        for (auto &interval : arr) {
            timeline[interval[0]]++;
            timeline[interval[1] + 1]--;
        }

        int current = 0, maximum = 0;

        for (auto &t : timeline) {
            current += t.second;
            maximum = max(maximum, current);
        }

        return maximum;
    }
};
